{
  "name": "ae86_chassis_all_terrain",//部件名称，必须与文件名相同，必须与文件名相同，必须与文件名相同
  "variants": {//部件变体列表，可有多个变体名和模型路径的键值对
    "default": "machine_max:part/ae86_chassis_all_terrain.geo"//若无变体，可缩减，即"variants": "machine_max:part/ae86_chassis_all_terrain.geo"
  },
  "textures": [//部件贴图列表，不同变体模型应共享一张贴图
    "machine_max:textures/part/ae86_1.png",
    "machine_max:textures/part/ae86_2.png",
    "machine_max:textures/part/ae86_3.png",
    "machine_max:textures/part/ae86_4.png"
  ],
  "basic_durability": 100.0,//部件基础生命值
  "subsystems": {
    "engine": {
      "type": "machine_max:engine", //子系统类型
      "max_power": 125000,//最大功率(W)
      "base_rpm": 500,//基础转速(转/分钟)
      "max_torque_rpm": 5000,//最大扭矩转速(转/分钟)
      "max_rpm": 7000,//最大转速(转/分钟)，超过此速度输出功率将严重衰减
      "throttle_inputs": ["move_output"],//可供转速输入的信号名称列表，优先级依次递减
      "speed_feedback_input": "speed_feedback",//转速反馈输入的信号名称
      "power_output_target": "left_front_wheel",//功率输出目标，可填写任意本部件内其他子系统名称、部件对接口名称，或“part”，或“vehicle”
      "rpm_output_targets": {//转速信号输出目标及信号名称，同上，但可以有多个
        "rot_speed": ["vehicle", "left_front_wheel", "right_front_wheel", "left_back_wheel", "right_back_wheel"]
      }
    },
    "transmission": {
      "type": "machine_max:transmission",
      "speed_input": "rot_speed",
      "speed_feedback_inputs": ["speed_feedback"],
      "speed_feedback_outputs": {
        "speed_feedback": ["engine"]
      },
      "power_outputs": {
        "left_front_wheel": {
          "speed_signal_output": "rot_speed",
          "power_weight": 1.0
        },
        "right_front_wheel": {
          "speed_signal_output": "rot_speed",
          "power_weight": 1.0
        },
        "left_back_wheel": {
          "speed_signal_output": "rot_speed",
          "power_weight": 1.0
        },
        "right_back_wheel": {
          "speed_signal_output": "rot_speed",
          "power_weight": 1.0
        }
      }
    },
    "left_front_wheel": {
      "type": "machine_max:motor",
      "connector": "left_front_wheel",
      "rotation_order": "XYZ",
      "axes": {
        "XR": {
          "needs_power": true,
          "max_force": 1000,
          "max_brake_force": 1500,
          "max_speed": 314,
          "target_speed_inputs": ["move_output"],
          "speed_outputs": {
            "speed_feedback": ["transmission"]
          }
        },
        "YR": {
          "needs_power": false,
          "max_force": 5000,
          "max_brake_force": 5000,
          "max_speed": 3.14,
          "target_speed_inputs": ["move_output"],
          "target_position_inputs": ["move_output"]
        }
      }
    },
    "right_front_wheel": {
      "type": "machine_max:motor",
      "connector": "right_front_wheel",
      "rotation_order": "XYZ",
      "axes": {
        "XR": {
          "needs_power": true,
          "max_force": 1000,
          "max_brake_force": 1500,
          "max_speed": 314,
          "target_speed_inputs": ["move_output"],
          "speed_outputs": {
            "speed_feedback": ["transmission"]
          }
        },
        "YR": {
          "needs_power": false,
          "max_force": 5000,
          "max_brake_force": 5000,
          "max_speed": 3.14,
          "target_speed_inputs": ["move_output"],
          "target_position_inputs": ["move_output"]
        }
      }
    },
    "left_back_wheel": {
      "type": "machine_max:motor",
      "connector": "left_back_wheel",
      "rotation_order": "XYZ",
      "axes": {
        "XR": {
          "needs_power": true,
          "max_force": 1000,
          "max_brake_force": 1500,
          "max_speed": 314,
          "target_speed_inputs": ["move_output"],
          "speed_outputs": {
            "speed_feedback": ["transmission"]
          }
        }
      }
    },
    "right_back_wheel": {
      "type": "machine_max:motor",
      "connector": "right_back_wheel",
      "rotation_order": "XYZ",
      "axes": {
        "XR": {
          "needs_power": true,
          "max_force": 1000,
          "max_brake_force": 1500,
          "max_speed": 314,
          "target_speed_inputs": ["move_output"],
          "speed_outputs": {
            "speed_feedback": ["transmission"]
          }
        }
      }
    }
  },
  "sub_parts": {//零件(子部件)列表
    "ae86_chassis_all_terrain": {//零件(子部件)名称
      "mass": 1000.0,//零件质量(kg)
      "mass_center": "MassCenter",//(可选)质心Locator名称，会寻找名称匹配的Locator，以其位置作为质心，留空则取坐标原点
      "shapes": {//键:骨骼名称，会寻找名称匹配的骨骼，并根据其中的方块创建不同形状的碰撞体
        "mmCollision_Box_Chassis": {//值:碰撞形状属性
          "type": "box",//碰撞形状类型，支持box、sphere、capsule、cylinder、cone
          "material": "default",//(可选)暂时无实际作用，计划影响摩擦力，伤害抗性，护甲等效系数等
          "thickness": 1 //(可选)暂时无实际作用，计划影响护甲水平
        }
      },
      "connectors": {//子部件用于与其他零件连接的对接口列表
        "left_front_wheel": {//接口名称
          "locator": "LeftFrontWheel",//Locator名称，会寻找名称匹配的Locator，以其位置与姿态作为对接口的位置和姿态
          "type": "Special",//对接口类型，支持AttachPoint和6DOF
          "variant": ["left"],//对接口可连接的部件变体类型，留空以接受所有类型的变体(例如轮胎的左右轮变体)
          "joint_attrs": {//关节属性，目前仅在关节类型为Special时生效
            "y": {
              "lower_limit": 1.0,
              "upper_limit": -1.0,
              "stiffness": 25000,//刚度系数(N/m)
              "damping": 1000 //阻尼系数(N/(m/s))
            },
            "xr": {//控制的轴，x、y、z分别对应x、y、z轴的平动，xr、yr、zr分别对应x、y、z轴的旋转
              "lower_limit": 1.0, //关节位置下限，对于平动轴单位为m，对于旋转轴单位为度
              "upper_limit": -1.0, //当下限高于上限时，指定的轴自由活动，下限低于上限时，指定的轴被限定于区间内活动，下线等于上限时，指定的轴被固定死
              "damping": 100
            },
            "yr": {
              "lower_limit": -45.0,
              "upper_limit": 45.0,
              "stiffness": 1000,
              "damping": 200
            }
          },
          "breakable": false //暂时无实际作用，计划用于设定对接口是否可被外力或伤害破坏
        },
        "left_back_wheel": {
          "locator": "LeftBackWheel",
          "type": "Special",
          "variant": ["left"],
          "joint_attrs": {
            "y": {
              "lower_limit": 1.0,
              "upper_limit": -1.0,
              "stiffness": 25000,
              "damping": 1000
            },
            "xr": {
              "lower_limit": 1.0,
              "upper_limit": -1.0,
              "damping": 100
            }
          }
        },
        "right_front_wheel": {
          "locator": "RightFrontWheel",
          "type": "Special",
          "variant": ["right"],
          "joint_attrs": {
            "y": {
              "lower_limit": 1.0,
              "upper_limit": -1.0,
              "stiffness": 25000,
              "damping": 1000
            },
            "xr": {
              "lower_limit": 1.0,
              "upper_limit": -1.0,
              "damping": 100
            },
            "yr": {
              "lower_limit": -45.0,
              "upper_limit": 45.0,
              "stiffness": 1000,
              "damping": 200
            }
          },
          "breakable": false
        },
        "right_back_wheel": {
          "locator": "RightBackWheel",
          "type": "Special",
          "variant": ["right"],
          "joint_attrs": {
            "y": {
              "lower_limit": 1.0,
              "upper_limit": -1.0,
              "stiffness": 25000,
              "damping": 1000
            },
            "xr": {
              "lower_limit": 1.0,
              "upper_limit": -1.0,
              "damping": 100
            }
          }
        },
        "hull": {
          "locator": "Hull",
          "type": "Special"
        },
        "driver_seat": {
          "locator": "DriverSeat",
          "type": "Special",
          "signal_targets": {
            "move_output": ["engine", "left_front_wheel", "right_front_wheel", "left_back_wheel", "right_back_wheel"]
          }
        },
        "copilot_seat": {
          "locator": "CopilotSeat",
          "type": "Special"
        },
        "back_seat": {
          "locator": "BackSeat",
          "type": "Special"
        }
      },
      "air_drag": {
        "front": 1.0,
        "back": 1.0,
        "left": 1.0,
        "right": 1.0,
        "up": 1.0,
        "down": 1.0
      }
    }
  }
}